{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "currentPassword": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "email": {
        "type": "string",
        "minLength": 6,
        "maxLength": 60,
        "format": "email"
      },
      "newPassword": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      }
    },
    "additionalProperties": false,
    "required": [
        "currentPassword"
    ]
  }